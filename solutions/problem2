def process_string(s):
    s = s[::-1]

    filtered = "".join(
        ch for i, ch in enumerate(s, start=1) if i % 3 != 0
    )

    shifted = "".join(chr(ord(c) + 2) for c in filtered)

    vowels = set("aeiouAEIOU")
    vowel_count = sum(1 for c in shifted if c in vowels)

    return vowel_count


def main():
    with open("inputs/input2.txt", "r") as f:
        text = f.read().strip()

    clue2 = process_string(text)
    print("Clue 2:", clue2)


if __name__ == "__main__":
    main()